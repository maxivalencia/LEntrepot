{% extends 'base.html.twig' %}

{% block title %}Typeproposition index{% endblock %}

{% block body %}
    <h1>Typeproposition index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typeproposition in typepropositions %}
            <tr>
                <td>{{ typeproposition.id }}</td>
                <td>{{ typeproposition.type }}</td>
                <td>
                    <a href="{{ path('typeproposition_show', {'id': typeproposition.id}) }}">show</a>
                    <a href="{{ path('typeproposition_edit', {'id': typeproposition.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('typeproposition_new') }}">Create new</a>
{% endblock %}
