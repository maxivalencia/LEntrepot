{# {% extends 'base.html.twig' %} #}

{% block title %}Typeprojet index{% endblock %}

{% block body %}
    <h1>Typeprojet index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typeprojet in typeprojets %}
            <tr>
                <td>{{ typeprojet.id }}</td>
                <td>{{ typeprojet.type }}</td>
                <td>
                    <a href="{{ path('typeprojet_show', {'id': typeprojet.id}) }}">show</a>
                    <a href="{{ path('typeprojet_edit', {'id': typeprojet.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('typeprojet_new') }}">Create new</a>
{% endblock %}
