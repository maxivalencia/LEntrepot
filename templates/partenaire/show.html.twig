{% extends 'base4.html.twig' %}

{% block title %}Information Partenaire{% endblock %}

{% block contenue %}
    <h1>Information Partenaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ partenaire.nom }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td><img src="/logo/{{ partenaire.logo }}" alt="logo {{ partenaire.nom }}" width="300px"></td>
                {# <td>{{ partenaire.logo }}</td> #}
            </tr>
        </tbody>
    </table>

    <a href="{{ path('partenaire_index') }}">Retour</a>

    <a href="{{ path('partenaire_edit', {'id': partenaire.id}) }}">Modifier</a>

    {{ include('partenaire/_delete_form.html.twig') }}
{% endblock %}
